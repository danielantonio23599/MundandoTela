<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.daniel.mundandotela.CalculadoraActivity">

    <LinearLayout
        android:layout_width="371dp"
        android:layout_height="501dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="2dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
            android:id="@+id/button10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClicBotao1"
            android:text="Calculadora1" />

        <Button
            android:id="@+id/button11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClicBotao3"
            android:text="Calculadora2" />

        <Button
            android:id="@+id/button12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClicBotao2"
            android:text="Calculadora3" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
